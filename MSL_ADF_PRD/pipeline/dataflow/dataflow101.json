{
	"name": "dataflow101",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AIA_AGENCY_EMP_HIER_DETAILS_HISTORY_UAT",
						"type": "DatasetReference"
					},
					"name": "dataflow102",
					"description": "Import data from AIA_AGENCY_EMP_HIER_DETAILS_HISTORY_PRD"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText102",
