				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "[MSL_998].[MSL_SP_ETL_AUDIT_UPDATE_BY_PIPELINE]",
					"storedProcedureParameters": {
						"PipelineRunId": {
							"value": {
								"value": "@pipeline().RunId",
								"type": "Expression"
							}
						},
						"Status": {
							"value": "Fails ",
							"type": "String"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "SqlServer_msldev",
					"type": "LinkedServiceReference"
				}
			}
		],
		"concurrency": 5,
		"parameters": {
