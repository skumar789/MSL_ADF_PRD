				"type": "SqlServerStoredProcedure",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "[MSL_998].[MSL_SP_ETL_DELETE_BY_FILERECORDBIRTHDATE]",
					"storedProcedureParameters": {
						"FileRecordBirthDate": {
							"value": {
								"value": "@pipeline().parameters.fileRecordBirthDate",
								"type": "Expression"
							},
							"type": "String"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "SqlServer_msldev",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "SP_MarkFailure_Truncate",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [
					{
						"activity": "DELETE_BY_FILERECORDBIRTHDATE",
						"dependencyConditions": [
							"Failed"
						]
